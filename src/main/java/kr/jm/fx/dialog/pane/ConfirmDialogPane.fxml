<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import kr.jm.fx.path.currentpathcombobox.CurrentPathComboBox?>
<?import kr.jm.fx.path.treetableview.PathListTreeTableView?>

<fx:root type="DialogPane" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<VBox>
			<children>
				<PathListTreeTableView fx:id="pathListTreeTableView" />
				<HBox>
					<children>
						<Button fx:id="changeButton" mnemonicParsing="false"
							text="%button.change.destination.directory" />
						<CurrentPathComboBox fx:id="currentPathComboBox"
							HBox.hgrow="ALWAYS" />
					</children>
				</HBox>
			</children>
		</VBox>
	</content>
</fx:root>
